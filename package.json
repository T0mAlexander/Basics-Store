{
  "name": "basic-store",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "npx eslint --ext *{.ts,.tsx} --ext src/*{.ts,.spec.ts} --ext src/**/*{.ts,.tsx} --ext src/**/**/*{.ts,.tsx} --ext src/**/*.tsx --ext test/*{.ts,.e2e-spec.ts} --fix",
    "nest:start": "nest start",
    "nest:build": "nest build",
    "nest:dev": "nest start --watch",
    "nest:debug": "nest start --debug --watch",
    "nest:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "test:e2e": "jest --config .test/jest-e2e.json"
  },
  "dependencies": {
    "@expo-google-fonts/nunito-sans": "^0.2.3",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "expo": "^48.0.16",
    "expo-navigation-bar": "~2.1.1",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-reanimated-carousel": "^3.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "styled-components": "^6.0.0-rc.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@prisma/client": "^4.14.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "@types/react": "^18.0.27",
    "@types/react-native": "~0.72.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
